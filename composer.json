	}
  },
  "extra": {
      "laravel": {
          "providers": [
              "Elibyy\\TCPDF\\ServiceProvider"
          ],
          "aliases": {
              "PDF": "Elibyy\\TCPDF\\Facades\\TCPDF"
          }
      }
  }
}
